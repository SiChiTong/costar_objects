<?xml version="0.0" ?>
<robot name="urdf_robot">

  <material name="red">
    <color rgba="0.8 0 0 1"/>
  </material>

  <link name="base_link">
    <contact>
      <rolling_friction value="0.005"/>
      <spinning_friction value="0.005"/>
    </contact>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <mass value=".1"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
    </collision>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <material name="red"/>
      <geometry>
        <sphere radius="0.02"/>
      </geometry>
    </visual>
  </link>
</robot>

